t()\r\n        .then(() => {\r\n        router.push(\"/\")\r\n      }).catch((error) => {\r\n        console.log(err)\r\n      })\r\n    }\r\n\r\n\r\n\r\n    return(\r\n        <FireContext.Provider value={{handleSignup, handleLogin, setEmail, setPassword, setFirstname,currentuser}} >\r\n            { children }\r\n        </FireContext.Provider>\r\n    )\r\n}\r\n"]},"metadata":{},"sourceType":"module"}